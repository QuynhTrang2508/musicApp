<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ImageButton">
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_marginBottom">@dimen/dp_40</item>
        <item name="android:layout_marginTop">@dimen/dp_380</item>
        <item name="android:layout_height">@dimen/dp_60</item>
        <item name="android:layout_width">@dimen/dp_60</item>
    </style>

    <style name="ImageButtonNotification">
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_marginBottom">@dimen/dp_40</item>
        <item name="android:layout_marginTop">@dimen/dp_40</item>
        <item name="android:layout_height">@dimen/dp_60</item>
        <item name="android:layout_width">@dimen/dp_60</item>
    </style>

    <style name="textStyle">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>
